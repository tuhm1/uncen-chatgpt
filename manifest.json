{
  "manifest_version": 3,
  "name": "Uncen ChatGPT",
  "description": "Prevent ChatGPT from hiding messages.",
  "version": "0.1.0",
  "content_scripts": [
    {
      "js": ["inject.js"],
      "matches": ["https://chatgpt.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["intercept.js"],
      "matches": ["https://chatgpt.com/*"]
    }
  ]
}
